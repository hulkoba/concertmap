    "test": "jest",
    "lint": "eslint src"
  },
  "dependencies": {
    "moment": "^2.17.1",
    "react": "15.4.1",
    "react-native": "0.42.0",
    "react-native-audio-streaming": "^2.3.1",
    "react-native-maps": "^0.12.4",
    "react-native-vector-icons": "^4.0.0"
  },
  "jest": {
    "preset": "jest-react-native"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-jest": "16.0.0",
    "babel-preset-react-native": "^1.9.1",
    "eslint": "3.12.1",
    "eslint-plugin-react": "^6.10.0",
    "jest": "18.0.0",
    "jest-react-native": "18.0.0",
    "react-native-dotenv": "0.0.3",
    "react-native-player": "0.0.9",
    "react-test-renderer": "15.3.2",
    "whatwg-fetch": "1.0.0"
  }
